<script language="javascript" type="text/javascript">
// CALL TO ACTION TRACKING
var IsCallToActionSet = false;
var ctaBtnClicked = null;
var ctaDtClicked = null;
var ckUrl = null;
function TrackCallToAction(sender, ctaName)
{
    try
    {
        if (sender != null && sender != ctaBtnClicked)
        {
            ctaBtnClicked = sender;
            ctaDtClicked = new Date();
	        var myUrl = 'http%3a%2f%2fit.toolbox.com%2fblogs%2fentry.aspx%3fslug%3dsecuritymonkey%26i%3d21691%26';
	        ckUrl = 'https://web.archive.org/web/20110114012349/http://it.toolbox.com/api/ctatools/CreateCookie.aspx?CTAPage=' + myUrl + '&CTA=' + ctaName;
        	
	        document.getElementById('ctaimage').src = ckUrl;
            // Give the image a second
            setTimeout("ctaBtnClicked.click()", 1500);
	    }
        if ((new Date() - ctaDtClicked) > 1499)//the wait is over
        {
            IsCallToActionSet = true;
        }
        return IsCallToActionSet;
    }
    catch(e){}
}
function CallToActionSet(el)
{
    if (el.src)
    {
        IsCallToActionSet = true;
        ctaBtnClicked.click();
    }
}
</script>
<img id="ctaimage" alt="" width="1" height="1" onload="CallToActionSet(this)"/>
    
    <div class="spacer-small"></div>
    <div align="center" style="margin-top:inherit;margin-bottom:5px">
	<script src="https://web.archive.org/web/20110114012349js_/http://ad.doubleclick.net/adj/security.ittoolbox/blogs;pos=boxlarge;role=0;cs=0;ind=0;kw=security;kw=casefile;kw=securitymonkey;kw=irc;kw=wireshark;cont=DM;sz=1x1,336x280,300x250,300x600;tile=7;ord=1896862965">

	</script>
