<script type="text/javascript">

var inMenu = false;
var globalHead = null;

function checkSearch() { var searchTerm = document.getElementById('r').value; if (searchTerm == '' || searchTerm == ' Search')  { alert('Please enter a search term.'); return false; } return true; }
function doSearchDropDown() { inMenu = true; getGlobalHeadMenu().style.display = 'block';}
function setInMenu() { inMenu = true; }

function getGlobalHeadMenu()
{
	if (! this.globalHead )
		this.globalHead = document.getElementById('globalheader_menu');
	return this.globalHead;
}
function hideSearchDropDown() { if (!inMenu) getGlobalHeadMenu().style.display = 'none'; }
function leaveMenu() { inMenu = false; setTimeout("hideSearchDropDown()", 1500); }

function setSearchHeader(sender) 
{
	var s = sender.innerHTML;
	var a = sender.attributes["action"];
	var r = sender.attributes["mainargumentname"]
	var n = sender.attributes["additionalargumentname"];
	var m = sender.attributes["additionalargumentvalue"];
	var text = document.getElementById('searchHeaderMainItem');
	if (navigator.appName == 'Microsoft Internet Explorer')
	{ // Internet Explorer removes the menu contents if we change the innerHTML of the list item, so use the inner span tag
		text = document.getElementById('searchHeaderMainItem_Text');
	}
	text.innerHTML = s;
	if (a) document.getElementById('globalheader_searchform').action = a.value; else document.getElementById('globalheader_searchform').action = 'https://web.archive.org/web/20100611102424/http://it.toolbox.com/home/search/';
	if (r) document.getElementById('r').name = r.value; else document.getElementById('r').name = 'r';
	if (n) document.getElementById('searchHeader_m').name = n.value; else document.getElementById('searchHeader_m').name = '';
	if (m) document.getElementById('searchHeader_m').value = m.value; else document.getElementById('searchHeader_m').value = '';
	inMenu = false;
	hideSearchDropDown();
}

</script>


<div id="ctl00_ctl00_ctl00_SearchHeader1_searchDiv" class="globalheader_searchdiv">
    <form id="globalheader_searchform" action="https://web.archive.org/web/20100611102424/http://it.toolbox.com/home/search/blogs/" method="GET" onsubmit="javascript:return checkSearch();">
        <input class="globalheader_input" name="r" id="r" value=" Search" type="text" onclick="this.value='';"/>
        <input type="hidden" id="searchHeader_m" name="" value=""/>
        <div class="globalheader_dropdown">
            <div class="globalheader_dropdownitem" style="cursor: pointer;" id="searchHeaderMainItem" onclick="javascript:doSearchDropDown();"><span id="searchHeaderMainItem_Text">Blogs</span></div>
            <div class="globalheader_dropdownup" id="globalheader_menu" style="display: none; position: absolute;">
            
					<div class="globalheader_menuitem" onclick="javascript:setSearchHeader(this);" action="http://it.toolbox.com/home/search/blogs/" mainargumentname="r" onmouseover="javascript:setInMenu();" onmouseout="javascript:leaveMenu();" style="border-top: solid 1px #9a9b9d;">Blogs</div>
				
					<div class="globalheader_menuitem" onclick="javascript:setSearchHeader(this);" onmouseover="javascript:setInMenu();" onmouseout="javascript:leaveMenu();">Toolbox for IT</div>
				
					<div class="globalheader_menuitem" onclick="javascript:setSearchHeader(this);" additionalargumentvalue="0" additionalargumentname="community" onmouseover="javascript:setInMenu();" onmouseout="javascript:leaveMenu();">Toolbox.com</div>
				
					<div class="globalheader_menuitem" onclick="javascript:setSearchHeader(this);" action="http://it.toolbox.com/home/search/white-papers/" onmouseover="javascript:setInMenu();" onmouseout="javascript:leaveMenu();">White Papers</div>
				
					<div class="globalheader_menuitem" onclick="javascript:setSearchHeader(this);" action="http://it.toolbox.com/home/search/communities/" onmouseover="javascript:setInMenu();" onmouseout="javascript:leaveMenu();">Communities</div>
				
					<div class="globalheader_menuitem" onclick="javascript:setSearchHeader(this);" action="http://it.toolbox.com/people/" mainargumentname="txtSearch" onmouseover="javascript:setInMenu();" onmouseout="javascript:leaveMenu();" style="border-bottom: solid 1px #9a9b9d;">People</div>
				
