    //GOOGLE ADS:
    var google_last_ad_type = '';
    function google_ad_request_done(google_ads) {
        var htmloutput;
        if (google_ads.length != 0) {
            htmloutput = '<div class="ad-title">Ads by Google</div>';
            htmloutput += '<a class="ad-attribution" href="' +
            google_info.feedback_url+'">what\'s this?</a><div class="clear"></div>';
            for (var i = 0; i < google_ads.length; i++) {
                htmloutput += '<br><a class="ad-link" href="' +
                google_ads[i].url +
                '" >' +
                google_ads[i].line1 +
                '</a><br><span class="black">' +
                google_ads[i].line2 + 
                google_ads[i].line3 + '</span><br>' +
                '<a href="' +
                google_ads[i].url + '" >' +
                google_ads[i].visible_url + '</a><br>';
            }
            document.write(htmloutput);
            return;
        }
    }
    //parameters of the ad call 
    google_ad_client = 'ca-ziffdavis_consumer_js'; // substitute your client_id
    google_ad_channel = 'toolbox'; // substitute your channel
    google_ad_output = 'js'; // leave this value as js
    google_max_num_ads = '5'; // specify the number of maximum ads
    google_ad_type = 'text'; // type of ads to display
    google_safe = 'high'; // specify the ad safety
    google_adtest = 'off';
</script>
