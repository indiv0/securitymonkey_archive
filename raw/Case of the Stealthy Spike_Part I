      color: '#ffffff',
      links: '#4aed05'
    }
  },
  features: {
    scrollbar: true,
    loop: false,
    live: true,
    hashtags: true,
    timestamp: true,
    avatars: true,
    behavior: 'default'
  }
}).render().setUser('chiefmonkey').start();
</script></span>        
        </div>        
        <div class="floatright">
            
        </div>
        <div class="clear"></div>
    
</div>
