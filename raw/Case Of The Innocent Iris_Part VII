<script language="javascript" type="text/javascript">
    function showHideMore(){
		var element;
		element = document.getElementById("lessVersion")
		element.style.display = (element.style.display == "none") ? "block" : "none";
		element.style.visible = (element.style.visible == "false") ? "true" : "false";
		element = document.getElementById("moreVersion")
		element.style.display = (element.style.display == "none") ? "block" : "none";
		element.style.visible = (element.style.visible == "false") ? "true" : "false";
	}
</script>
<script type="text/javascript">
// CALL TO ACTION TRACKING
var IsCallToActionSet = false;
var ctaBtnClicked = null;
var ctaDtClicked = null;
var ckUrl = null;
function TrackCallToAction(sender, ctaName)
{
    try
    {
        if (sender != null && sender != ctaBtnClicked)
        {
            ctaBtnClicked = sender;
            ctaDtClicked = new Date();
	        var myUrl = 'http%3a%2f%2fit.toolbox.com%2fblogs%2fblogs%2fentry.aspx%3fslug%3dsecuritymonkey%26i%3d19684%26';
	        ckUrl = 'https://web.archive.org/web/20100504133803/http://it.toolbox.com/api/ctatools/CreateCookie.aspx?CTAPage=' + myUrl + '&CTA=' + ctaName;
        	
	        document.getElementById('ctaimage').src = ckUrl;
            // Give the image a second
            setTimeout("ctaBtnClicked.click()", 1500);
	    }
        if ((new Date() - ctaDtClicked) > 1499)//the wait is over
        {
            IsCallToActionSet = true;
        }
        return IsCallToActionSet;
    }
    catch(e){}
}
function CallToActionSet(el)
{
    if (el.src)
    {
        IsCallToActionSet = true;
        ctaBtnClicked.click();
    }
}
</script>
<img id="ctaimage" alt="" width="1" height="1" onload="CallToActionSet(this)"/>



